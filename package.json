{
  "name": "arc-config-compiler",
  "version": "1.0.4",
  "description": "Compile multiple arc files into one arc config",
  "main": "dist/index,js",
  "repository": {
    "type": "git",
    "url": "https://github.com/arthur-rl/arc-config-compiler.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "tsc -p . && node dist/index.js --input ./input/** --output ./app.arc"
  },
  "bin": {
    "arc-compile": "dist/index.js"
  },
  "keywords": [
    "architect",
    "arc.codes",
    "arc"
  ],
  "author": "arthur-rl",
  "license": "MIT",
  "dependencies": {
    "@architect/parser": "^5.0.2",
    "arg": "^5.0.1",
    "chalk": "^4.1.2",
    "glob": "^7.2.0",
    "node": "^17.3.0"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/glob": "^7.2.0",
    "@types/node": "^17.0.4"
  }
}
